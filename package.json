    "fxjs2": "^0.2.1",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.0",
    "@types/morgan": "^1.7.35",
    "@types/shelljs": "^0.8.2",
    "@types/supertest": "^2.0.7",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "shelljs": "^0.8.3",
    "supertest": "^3.4.2"
