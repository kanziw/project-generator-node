{
  "name": "project-generator-node",
  "version": "0.1.0",
  "description": "Project generator for Node.js",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kanziw/project-generator-node"
  },
  "author": "{{githubProfileOrOrganizationName}} <{{email}}>",
  "license": "MIT",
  "scripts": {
    "postinstall": "node ./generator/postInstall.js",
    "lint": "eslint src/**",
